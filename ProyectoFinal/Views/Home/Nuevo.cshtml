@model Publicacion
@{
    ViewData["Title"] = "Nuevo Post";
    ViewData["Image"] = "/assets/img/Contact-bg.jpg";
    ViewData["TitleHeader"] = "Nuevo";
    ViewData["SubTitleHeader"] = "Nuevo";
}
<!-- Main Content-->
<div class="container px-4 px-lg-5">
    <div class="row gx-4 gx-lg-5 justify-content-center">
        <div class="col-md-10 col-lg-8 col-xl-7">

            
            <div class="my-5">
                <p>
                    Complete el formulario para dar de alta un artículo
                </p>
                <form asp-action="Add">
                    <partial name="_formPublicacion" model="@Model" />
                    <button type="submit" value="Contacto" class="btn btn-primary text-uppercase disabled">Crear Post</button>
                </form>    
            </div>

        </div>
    </div>
</div>