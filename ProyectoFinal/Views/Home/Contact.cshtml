@model Contacto
@{
    ViewData["Title"] = "Contact";
    ViewData["Image"] = "/assets/img/contact-bg.jpg";
    ViewData["TitleHeader"] = "Contacto";
    ViewData["SubTitleHeader"] = "Contactanos";
}
<!-- Main Content-->
<div class="container px-4 px-lg-5">
    <div class="row gx-4 gx-lg-5 justify-content-center">
        <div class="col-md-10 col-lg-8 col-xl-7">

            <p>Want to get in touch? Fill out the form below to send me a message and I will get back to you as soon as possible!</p>
            <div class="my-5">
                <p>
                    Complete el formulario y nos pondremos en contacto a la brevedad
                </p>
                <form asp-action="Contacto">
                    <partial name="_formContacto" model="@Model" />
                    <button type="submit" value="Contacto" class="btn btn-primary text-uppercase disabled">Enviar Consulta</button>
                </form>    
            </div>

        </div>
    </div>
</div>
